<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <!-- ================================================================== -->
    <SourceProxy name="StratifiedSampling" class="vtkStratifiedSampling" label="StratifiedSampling">
      <Documentation
         long_help="Uniformly resample a mesh."
         short_help="Uniformly resample a mesh.">
      </Documentation>

      <InputProperty
         name="Input"
         command="AddInputConnection"
         clean_command="RemoveAllInputs">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData"/>
        </DataTypeDomain>
        <Documentation>
          Set the input point set.
        </Documentation>
      </InputProperty>

      <IntVectorProperty
         name="Level"
         command="SetLevel"
         number_of_elements="1"
         default_values="6">
      </IntVectorProperty>
         
      <DoubleVectorProperty
         name="Lambda"
         command="SetLambda"
         number_of_elements="1"
         default_values="10.0">
      </DoubleVectorProperty>
         
      <DoubleVectorProperty
         name="Bad"
         command="SetBad"
         number_of_elements="1"
         default_values="0.5">
      </DoubleVectorProperty>

      <IntVectorProperty
         name="RandomSeed"
         command="SetRandomSeed"
         number_of_elements="1"
         default_values="0">
      </IntVectorProperty>
      
    </SourceProxy>
    <!-- End StratifiedSampling-->
  </ProxyGroup>
  <!-- End Filters Group -->
</ServerManagerConfiguration>
